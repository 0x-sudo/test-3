{
  "name": "cosmos",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "postinstall": "npm run typegen",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "generate": "drizzle-kit generate:sqlite",
    "drop": "drizzle-kit drop",
    "migrate:local": "wrangler d1 migrations apply cosmos_db --local",
    "migrate:prod": "wrangler d1 migrations apply cosmos_db",
    "typecheck": "tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.2",
    "@conform-to/zod": "^1.0.2",
    "@epic-web/totp": "^1.1.2",
    "@libsql/client": "^0.5.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-run/cloudflare": "^2.7.1",
    "@remix-run/cloudflare-pages": "^2.7.1",
    "@remix-run/react": "^2.7.1",
    "dotenv-cli": "^7.3.0",
    "drizzle-orm": "^0.29.3",
    "isbot": "^5.1.0",
    "miniflare": "^3.20240129.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-env-only": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@remix-run/dev": "^2.7.1",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-tsconfig-paths": "^4.3.1",
    "wrangler": "^3.28.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
